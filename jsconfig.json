{
  "compilerOptions": {
    "baseUrl": ".",
    "lib": ["DOM", "DOM.Iterable", "ES2022"],
    "noEmit": true,
    "esModuleInterop": true,
    "paths": {
      "@parts/*": ["components/parts/*"],
      "@sections/*": ["components/sections/*"],
      "@data/*": ["data/*"],
      "@lib/*": ["lib/*"],
      "@hooks/*": ["hooks/*"],
      "@styles/*": ["styles/*"],
      "@typography/*": ["components/typography/*"]
    }
  },
  "include": ["next-env.d.js", "**/*.js", "**/*.jsx"],
  "exclude": ["node_modules"]
}
